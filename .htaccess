#True rewrite engine on 
Option +FollowSymLinks
RewriteEngine On

#map neat URL to internal URL 
RewriteRule ^/fishingrod /list/$index.php?view=all [nc,qsa]
RewriteRule ^/fishingrod/([0-9]+)/$ $index.php?view=single&id=$1 [nc,qsa]
RewriteRule ^/fishingrod/type/([a-zA-ZáéiíöőúüűÁÉÍÖŐÚÜŰ]+)/$ $index.php?view=type&tid=$1 [nc,qsa]
RewriteRule ^/fishingrod/.*$ $index.php [nc,qsa]


